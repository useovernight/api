{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "pagination": {
      "type": "library",
      "root": "libs/pagination",
      "entryFile": "index",
      "sourceRoot": "libs/pagination/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pagination/tsconfig.lib.json"
      }
    },
    "document-transfer": {
      "type": "library",
      "root": "libs/document-transfer",
      "entryFile": "index",
      "sourceRoot": "libs/document-transfer/src",
      "compilerOptions": {
        "tsConfigPath": "libs/document-transfer/tsconfig.lib.json"
      }
    }
  }
}
