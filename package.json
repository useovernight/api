{
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "prepare": "husky install",
    "build": "nest build",
    "clean": "rimraf ./dist",
    "start": "cross-env NODE_ENV=dev nest start",
    "start:dev": "yarn run start --watch",
    "start:debug": "yarn run start --debug --watch",
    "start:prod": "node dist/main",
    "format": "prettier --write \"{src,apps,libs,test,scripts,database}/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test,scrips,database}/**/*.ts\"",
    "lint:fix": "yarn run lint --fix",
    "test": "jest",
    "test:watch": "yarn run test --watch",
    "test:cov": "yarn run test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=dev yarn run test --config ./test/jest-e2e.json",
    "x": "DOTENV_CONFIG_PATH=.env.dev node -r ts-node/register -r dotenv/config",
    "typeorm": "yarn run x ./node_modules/typeorm/cli.js -d ./typeorm.data-source.ts",
    "typeorm:migration:generate": "yarn run typeorm migration:generate",
    "typeorm:migration:run": "yarn run typeorm migration:run",
    "typeorm:migration:revert": "yarn run typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ms": "^2.1.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/ms": "^0.7.31",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^14.0.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@3.6.3"
}
